{
    "name": "former-model-theme",
    "version": "1.0.0",
    "description": "Custom FSE theme with Webpack build",
    "scripts": {
        "build": "webpack --mode development",
        "dist": "webpack --mode production",
        "watch": "npm-run-all --parallel watch:theme watch:webpack",
        "watch:theme": "node watch-theme-json.js",
        "watch:webpack": "webpack --watch --mode development"
    },
    "devDependencies": {
        "@babel/core": "^7.23.8",
        "@babel/preset-env": "^7.23.8",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^5.0.1",
        "mini-css-extract-plugin": "^2.7.6",
        "npm-run-all": "^4.1.5",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
}