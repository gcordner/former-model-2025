{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 3,
    "settings": {
        "layout": {
            "contentSize": "750px",
            "wideSize": "1200px"
        },
        "color": {
            "custom": true,
            "palette": []
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "%",
                "vh",
                "vw"
            ]
        },
        "custom": {
            "shrinkHeaderOffset": "50px"
        }
    },
    "styles": {
        "typography": {
            "fontSize": "16px",
            "lineHeight": "1.6"
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}